{
  "name": "panda-tech-cli",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky",
    "start": "node ./build/index.js",
    "dev": "nodemon",
    "develop": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./src/index.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "build": "swc src -d build --strip-leading-paths",
    "build:watch": "swc ./src -d build -w",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "prettier": "prettier '**/*.{ts,json,md}'",
    "prettier:write": "prettier --write '**/*.{ts,json,md}'",
    "type-check": "tsc --noEmit",
    "lint-staged": "lint-staged",
    "release": "semantic-release",
    "bundle": "unbuild"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.5.2",
  "dependencies": {
    "@clack/prompts": "^0.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@swc/cli": "^0.7.3",
    "@swc/core": "^1.11.22",
    "eslint": "^9.25.1",
    "eslint-config-xo": "^0.46.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0"
  }
}
